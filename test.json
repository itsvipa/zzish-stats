{
  "$jason": {
    "head": {
      "title": "Zzish",
      "actions": {
        "visit": [
          {
            "{{#if /\\/edit/.test($zzish.url)}}": {
              "type": "$href",
              "options": {
                "url": "{{$jason.url.zzish('/edit', '')}}",
                "preload": {
                  "background": "#ffffff"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$default"
            }
          }
        ]
      }
    },
    "body": {
      "background": {
        "type": "html",
        "url": "https://zzi.sh",
        "action": {
          "trigger": "visit"
        }
      }
    }
  }
}
